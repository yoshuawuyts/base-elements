{
  "name": "base-elements",
  "version": "2.2.1",
  "description": "A selection of native DOM elements",
  "main": "index.js",
  "scripts": {
    "start": "bankai start example/index.js --open",
    "build": "bankai build example/index.js",
    "deps": "dependency-check . && dependency-check . -i balloon-css --extra --no-dev",
    "test": "standard && npm run deps",
    "test:cov": "standard && npm run deps"
  },
  "browserify": {
    "transform": [
      "sheetify/transform"
    ]
  },
  "repository": "yoshuawuyts/base-elements",
  "keywords": [
    "dom",
    "native",
    "choo",
    "bel",
    "yo-yo"
  ],
  "license": "MIT",
  "dependencies": {
    "balloon-css": "^0.4.0",
    "bel": "^4.5.0",
    "nanocomponent": "^2.0.0",
    "sheetify": "^6.0.1",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "append-child": "^1.0.0",
    "bankai": "^5.2.1",
    "choo": "^5.3.0",
    "dependency-check": "^2.6.0",
    "gh-pages": "^0.12.0",
    "insert-css": "^1.0.0",
    "sheetify": "^5.1.1",
    "standard": "^8.3.0",
    "tachyons": "^4.5.3",
    "tape": "^4.6.2"
  }
}
